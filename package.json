{
  "name": "talleye",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "npx prettier --check . && npx eslint .",
    "lint": "eslint .",
    "prettify": "prettier --write .",
    "postinstall": "husky install"
  },
  "dependencies": {
    "next": "10.0.5",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "gray-matter": "^4.0.2",
    "husky": "^4.3.8",
    "prettier": "2.2.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  }
}
